#creo la prima tabella concessionari

drop table if exists concessionari;
create table concessionari(
        Id integer not null, 
        Nome character(20) not null,
	Indirizzo character(20) not null,
	Civico character(5) ,
	Città character(20),
	Provincia character(2),
	CAP integer not null,	
	Web character(50),
	Telefono character(20),
	Fax character(20) ,
	Latitudine decimal not null,
	Longitudine decimal not null

);

#creo la tabella che contiene i modelli di auto venduti da ciascun rivenditore
#contiente una chiave per la tabella concessionari e una per la tabella auto.

drop table if exist  vendita;
create table vendita(
  IdConcessionario integer not null,
  NomeAuto character(20) not null
);


